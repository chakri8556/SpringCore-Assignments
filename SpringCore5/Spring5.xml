<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
    
    <context:annotation-config></context:annotation-config>
   
    <bean id="customer" class="assign5.Customer">
    <property name="customerName" value="chakri"></property>
    <property name="customerId" value="15"></property>
    <property name="customerContact" value="6300526561"></property>
    </bean>
    <bean id="address" class="assign5.Adress">
    <property name="street" value="kn street"></property>
    <property name="city" value="kkd"></property>
    <property name="state" value="AP"></property>
    <property name="zip" value="533016"></property>
    <property name="country" value="India"></property>
    </bean>
    
    
    
    </beans>